<!DOCTYPE html>
<html>
<head>
  <title>Maframe</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

	<div class="fm-navbar">
		<div class="fm-navbar-inner">
				<a class="brand" href="/">Ma Frame</a>
		</div>
	</div>
	<div class="fm-container">
	<%= content_tag(:div, notice, class: "fm-notice") if notice %>
	<%= content_tag(:div, alert, class: "fm-alert") if alert %>
	<%= yield %>
	</div>

</body>
</html>
